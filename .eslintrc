{
    "env": {
        "browser": true,
        "node":true,
        "es2021": true,
        "jest/global": true
    },
    "extends": "eslint:recommended",
    "overrides": [
        {
            "env": {
                "node": true
            },
            "files": [
                ".eslintrc.{js,cjs}"
            ],
            "parserOptions": {
                "sourceType": "script"
            }
        }
    ],
    "languageOptions": {
        "globals":"...globals.browser"
        
    },
    "plugins":["jest"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
    },
    "max-len": [
        "error",
        {
          "code": 120,
          "comments": 65,
          "tabWidth": 2,
          "ignoreUrls": true,
          "ignoreTemplateLiterals": true,
          "ignoreRegExpLiterals": true
        }
      ]
}
